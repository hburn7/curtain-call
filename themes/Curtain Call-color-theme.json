  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5a5a5a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#d4d64b"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#deac40"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7976db"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#afb171"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#e1b633"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#49d053"
      }
    },
    {
      "scope": ["support.function", "support.variable", "support.constant"],
      "settings": {
        "foreground": "#afb171"
      }
    },
    {
      "scope": ["constant.other", "constant.character"],
      "settings": {
        "foreground": "#deac40"
      }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e1b633",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#afb171",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#5a5a5a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#d4d64b"
      }
    },
    {
      "scope": ["meta.selector", "entity.other.attribute-name.css"],
      "settings": {
        "foreground": "#49d053"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#afb171"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#e1b633"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#939393"
      }
    }
  ]
}
